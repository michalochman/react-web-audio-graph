  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "8.10.0",
    "gh-pages": "3.2.3",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{ts,tsx,json,css,md}": "prettier --write"
  }
}
